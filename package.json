{
  "name": "v-mark-docs",
  "description": "Markdown related utilities for documenting Vue components.",
  "version": "0.0.21",
  "keywords": [
    "vue",
    "markdown",
    "documentation"
  ],
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sq11y/v-mark-docs"
  },
  "files": [
    "dist/",
    "env.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": "./env.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --fix",
    "format": "prettier --write ./src"
  },
  "peerDependencies": {
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@mdit-vue/plugin-component": "^3.0.2",
    "@mdit-vue/plugin-frontmatter": "^3.0.2",
    "@mdit-vue/plugin-sfc": "^3.0.2",
    "fast-glob": "^3.3.3",
    "find-up": "^8.0.0",
    "front-matter": "^4.0.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-vue-meta": "^0.0.1",
    "v-custom-block": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "jiti": "^2.6.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.44.1"
  }
}
